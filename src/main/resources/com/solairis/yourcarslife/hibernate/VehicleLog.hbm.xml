<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.solairis.yourcarslife.data.domain">
	<class name="VehicleLog" table="LOG">
		<id name="vehicleLogId" column="LOG_ID">
			<generator class="native"/>
		</id>
		<property name="odometer" column="ODOMETER"/>
		<property name="created" type="timestamp" column="RECORD_CREATED"/>
		<property name="modified" type="timestamp" column="RECORD_MODIFIED"/>
	</class>
</hibernate-mapping>