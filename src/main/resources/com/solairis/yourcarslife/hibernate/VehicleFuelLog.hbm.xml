<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.solairis.yourcarslife.data.domain">
  <class name="VehicleFuelLog" table="vehicleFuelLog" lazy="false">
    <id name="vehicleFuelLogId">
      <generator class="native"/>
    </id>
    <property name="odometer"/>
    <property name="created" type="timestamp"/>
    <property name="modified" type="timestamp"/>
	<property name="logDate" type="timestamp"/>
	<property name="fuel"/>
	<property name="octane"/>
	<property name="active" type="boolean"/>
  </class>
</hibernate-mapping>